,0)"><rect stroke="none" width="1236" height="60" x="0" y="220"></rect><use xlink:href="#E72-MJMAIN-31" x="368" y="676"></use><g transform="translate(60,-686)"><use xlink:href="#E72-MJMATHI-61" x="0" y="0"></use><g transform="translate(529,-150)"><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-69" x="0" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-69" x="345" y="0"></use></g></g></g></g><use xlink:href="#E72-MJMAIN-28" x="7567" y="0"></use><g transform="translate(7956,0)"><use xlink:href="#E72-MJMATHI-62" x="0" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-69" x="606" y="-213"></use></g><use xlink:href="#E72-MJMAIN-2212" x="8951" y="0"></use><g transform="translate(9951,0)"><use xlink:href="#E72-MJSZ2-2211" x="0" y="0"></use><g transform="translate(124,-1088)"><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-6A" x="0" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMAIN-3D" x="412" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMAIN-31" x="1189" y="0"></use></g><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-6E" x="721" y="1626"></use></g><g transform="translate(11562,0)"><use xlink:href="#E72-MJMATHI-61" x="0" y="0"></use><g transform="translate(529,-150)"><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-69" x="0" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-6A" x="345" y="0"></use></g></g><g transform="translate(12726,0)"><use xlink:href="#E72-MJMATHI-78" x="0" y="0"></use><g transform="translate(572,521)"><use transform="scale(0.707)" xlink:href="#E72-MJMAIN-28" x="0" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-6B" x="389" y="0"></use><use transform="scale(0.707)" xlink:href="#E72-MJMAIN-29" x="909" y="0"></use></g><use transform="scale(0.707)" xlink:href="#E72-MJMATHI-6A" x="808" y="-429"></use></g><use xlink:href="#E72-MJMAIN-29" x="14317" y="0"></use><use xlink:href="#E72-MJMAIN-2C" x="14706" y="0"></use><use xlink:href="